<h1>Все наши тесты</h1>
<br>

<% @test.each do |test| %>
  <div class="alert alert-light">
    <h3>Тест: <%= test.title %></h3>
    <p>Уровень: <%= test.level %></p>
    <p>Количество вопросов: <%= test.questions.count %></p>
    <% if user_signed_in? %>
      <%= button_to "Pass_the_test", start_test_path(test), :class => 'btn btn-success' %>
    <% end %>
  </div>
  <hr>
<% end %>
