<% @test.questions.each_with_index do |question, i| %>
	<p>
		<%= i + 1 %>. <b>Вопрос: </b> <%= question.body %>
		<%= link_to 'Show', admin_question_path(question), :class => "btn btn-warning" %>
	</p>
<% end %>
<% if user_signed_in? %>
	<%= link_to 'Add new', new_test_question_path(@test.id), :class => "btn btn-warning" %>
<% end %>
<%= link_to 'Back', admin_tests_path, :class => "btn btn-warning" %>
<hr>
