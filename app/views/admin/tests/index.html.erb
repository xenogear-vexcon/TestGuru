<div class="row justify-content-center mt-3">
  <div class="col col-md-10">
    <h1><%= t('.header') %></h1>
    <br>
    <%= link_to t('.gists'), admin_gists_path, class: 'btn btn-sm btn-outline-secondary' %>
    <%= link_to t('.new'), new_admin_test_path, class: 'btn btn-sm btn-outline-secondary' %>
    <%= link_to t('.badges'), admin_badges_path, class: 'btn btn-sm btn-outline-secondary' %>
    <%= render 'shared/errors', resource: @test %>
    <br><br>
    <table class="table">
      <thead>
      <tr>
        <th scope="col" class="sort-by-title pointer">
          <%= t('.title') %>
          <%= octicon 'arrow-up', class: 'text-success hide' %>
          <%= octicon 'arrow-down', class: 'text-success hide' %>
        </th>
        <th scope="col"><%= t('.level') %></th>
        <th scope="col"><%= t('.questions') %></th>
        <th colspan="4" scope="col" class="text-center"><%= t('.actions') %></th>
      </tr>
      </thead>

    <%= render @tests %>

    </table>
  </div>
</div>